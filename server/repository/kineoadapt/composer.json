{
    "name": "tke/repository-kineo-adapt",
    "type": "totara-repository",
    "description": "A Totara repository plugin to enable scorm activities published in external Adapt Authoring Tool sites to be added to courses in Totara.",
    "require": {
        "kineo/installers": "~1.3",
        "tke/tke": "~15.0"
    },
    "extra": {
        "installer-name": "kineoadapt",
        "kineo": {
            "code-standards": "moodle",
            "has-config-init": false,
            "has-core-modifications": false,
            "has-htaccess": false,
            "has-totara-component": false
        }
    },
    "support": {
        "wiki": "https://gitlab.kineodev.com/plugins/repository-kineo-adapt/wikis/home"
    }
}
