{
    "name": "tke/auth-simplesamlidp",
    "type": "totara-auth",
    "description": "Authentication plugin to enable Totara to act as an Identity Provider using SAML (SimpleSAMLphp)",
    "require": {
        "kineo/installers": "~1.3",
        "tke/tke": "~15.0"
    },
    "extra": {
        "installer-name": "simplesamlidp",
        "kineo": {
            "code-standards": "moodle",
            "has-config-init": false,
            "has-core-modifications": false,
            "has-htaccess": false,
            "has-totara-component": false
        }
    },
    "support": {
        "wiki": "https://gitlab.kineodev.com/plugins/auth-simplesamlidp/wikis/home"
    }
}
