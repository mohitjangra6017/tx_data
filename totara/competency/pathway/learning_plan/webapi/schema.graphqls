type pathway_learning_plan_competency_plan {
  learning_plans: [totara_plan_learning_plan] # learning plans this competency is in
  scale_value: totara_competency_scale_value # value given
  date_assigned(format: core_date_format = TIMESTAMP): core_date # when the value was given
}

extend type Query {
  pathway_learning_plan_competency_plans(
    user_id: core_id!
    assignment_id: core_id!
  ): pathway_learning_plan_competency_plan
}
