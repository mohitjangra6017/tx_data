{
    "name": "totaraenterprise",
    "private": true,
    "description": "Totara Enterprise",
    "scripts": {
        "tui-build": "webpack --mode production && webpack --mode development",
        "tui-build-prod": "webpack --mode production",
        "tui-build-dev": "webpack --mode development",
        "tui-build-watch": "webpack --mode development --watch",
        "tui-style-check": "node ./client/scripts/tasks/lint.js --formatting",
        "tui-style-fix": "node ./client/scripts/tasks/lint.js --fix --formatting",
        "tui-style-format": "node ./client/scripts/tasks/prettier.js --write",
        "tui-test": "jest && cross-env BABEL_LEGACY=1 jest",
        "tui-test-watch": "jest --watch",
        "tui-ci": "npm run tui-build && npm run tui-test && npm run tui-style-check",
        "setup-hooks": "node ./client/scripts/tasks/setup-hooks.js",
        "remove-hooks": "node ./client/scripts/tasks/remove-hooks.js",
        "generate-configs": "node ./client/scripts/tasks/generate-configs.js",
        "tui-init": "node ./client/scripts/tasks/tui-init.js"
    },
    "@comment dependencies": [
        "Dependencies are used by webpack to bundle libraries that will be delievered to the client",
        "as part of the vendor bundle. Dependencies need to be locked to a single version for this reason",
        "and upgrades need to adhere to our third party library policy.",
        "Details and licenses:",
        " - apollo-cache-inmemory",
        "       https://www.npmjs.com/package/apollo-cache-inmemory - MIT",
        " - apollo-client",
        "       https://www.npmjs.com/package/apollo-client - MIT",
        " - apollo-link",
        "       https://www.npmjs.com/package/apollo-link - MIT",
        " - apollo-link-batch-http",
        "       https://www.npmjs.com/package/apollo-link-batch-http - MIT",
        " - apollo-link-http",
        "       https://www.npmjs.com/package/apollo-link-http - MIT",
        " - chart.js",
        "       https://www.npmjs.com/package/chart.js - MIT",
        " - chartjs-plugin-doughnutlabel",
        "       https://www.npmjs.com/package/chartjs-plugin-doughnutlabel - MIT (ISC)",
        " - graphql",
        "       https://www.npmjs.com/package/graphql - MIT",
        " - graphql-tag",
        "       https://www.npmjs.com/package/graphql-tag - MIT",
        " - resize-observer-polyfill",
        "       https://www.npmjs.com/package/vue - MIT",
        " - vue",
        "       https://www.npmjs.com/package/vue - MIT",
        " - vue-apollo",
        "       https://www.npmjs.com/package/vue-apollo - MIT"
    ],
    "dependencies": {
        "apollo-cache-inmemory": "^1.6.2",
        "apollo-client": "^2.6.3",
        "apollo-link": "^1.2.12",
        "apollo-link-batch-http": "^1.2.13",
        "apollo-link-http": "^1.5.16",
        "chart.js": "^2.8.0",
        "chartjs-plugin-doughnutlabel": "^2.0.3",
        "date-fns": "^2.13.0",
        "graphql": "^14.4.2",
        "graphql-tag": "^2.10.1",
        "resize-observer-polyfill": "^1.5.1",
        "vue": "^2.6.10",
        "vue-apollo": "^3.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.5.4",
        "@babel/preset-env": "^7.5.4",
        "@vue/test-utils": "1.0.0-beta.33",
        "autoprefixer": "^9.7.3",
        "babel-eslint": "^10.0.2",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-async-to-promises": "^0.8.15",
        "concurrently": "^4.1.1",
        "cross-env": "^6.0.3",
        "eslint": "6.7.1",
        "eslint-config-prettier": "6.7.0",
        "eslint-loader": "^3.0.2",
        "eslint-plugin-jest": "23.0.5",
        "eslint-plugin-promise": "3.5.0",
        "eslint-plugin-tui": "file:client/scripts/eslint/plugin",
        "eslint-plugin-vue": "5.2.3",
        "jest": "^25.1.0",
        "jest-axe": "^3.4.0",
        "jest-canvas-mock": "^2.1.0",
        "jest-extended": "^0.11.5",
        "jest-junit": "^10.0.0",
        "jest-serializer-vue": "^2.0.2",
        "jest-transform-stub": "^2.0.0",
        "jest-watch-typeahead": "^0.4.0",
        "license-checker": "^25.0.1",
        "lint-staged": "^9.5.0",
        "loader-utils": "^1.2.3",
        "mini-css-extract-plugin": "^0.8.0",
        "minimatch": "^3.0.4",
        "null-loader": "^3.0.0",
        "postcss": "^7.0.29",
        "postcss-loader": "^3.0.0",
        "postcss-scss": "^2.0.0",
        "prettier": "1.19.1",
        "run-node": "^1.0.0",
        "semver": "^6.3.0",
        "source-list-map": "^2.0.1",
        "stylelint": "12.0.0",
        "stylelint-checkstyle-formatter": "0.1.2",
        "stylelint-config-prettier": "7.0.0",
        "stylelint-order": "3.1.1",
        "tiny-glob": "^0.2.6",
        "vue-jest": "4.0.0-beta.2",
        "vue-loader": "^15.7.0",
        "vue-template-compiler": "^2.6.10",
        "webpack": "4.41.2",
        "webpack-cli": "^3.3.9",
        "webpack-fix-style-only-entries": "^0.4.0",
        "yargs": "^15.0.2"
    },
    "engines": {
        "node": ">=12"
    }
}
