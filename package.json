{
    "name": "totaralms",
    "private": true,
    "description": "Totara LMS",
    "scripts": {
        "watch-all": "concurrently \"npm run tui-watch\" \"grunt watch\"",
        "tui-dev": "webpack --mode development",
        "tui-watch": "webpack --mode development --watch",
        "tui-prod": "webpack --mode production && npm run test-unit && npm run test-unit-legacy && npm run lint",
        "lint": "node ./totara/core/tui/scripts/tasks/lint.js",
        "lintc": "node ./totara/core/tui/scripts/tasks/lint.js --changed",
        "format": "node ./totara/core/tui/scripts/tasks/prettier.js --write",
        "formatc": "node ./totara/core/tui/scripts/tasks/prettier.js --write --changed",
        "test-unit": "jest",
        "test-unit-watch": "jest --watch",
        "test-unit-legacy": "cross-env BABEL_LEGACY=1 jest",
        "test-unit-snapshot": "jest --updateSnapshot",
        "setup-hooks": "node ./totara/core/tui/scripts/tasks/setup-hooks.js",
        "remove-hooks": "node ./totara/core/tui/scripts/tasks/remove-hooks.js",
        "generate-configs": "node ./totara/core/tui/scripts/tasks/generate-configs.js",
        "tui-init": "node ./totara/core/tui/scripts/tasks/tui-init.js"
    },
    "dependencies": {
        "apollo-cache-inmemory": "^1.6.2",
        "apollo-client": "^2.6.3",
        "apollo-link": "^1.2.12",
        "apollo-link-batch-http": "^1.2.13",
        "apollo-link-http": "^1.5.16",
        "graphql": "^14.4.2",
        "graphql-tag": "^2.10.1",
        "vue": "^2.6.10",
        "vue-apollo": "^3.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.5.4",
        "@babel/preset-env": "^7.5.4",
        "@vue/test-utils": "1.0.0-beta.30",
        "async": "1.5.2",
        "autoprefixer": "^6.3.7",
        "babel-eslint": "^10.0.2",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-async-to-promises": "^0.8.15",
        "concurrently": "^4.1.1",
        "cross-env": "^6.0.3",
        "css-loader": "^2.1.1",
        "eslint": "6.7.1",
        "eslint-config-prettier": "6.7.0",
        "eslint-plugin-promise": "3.5.0",
        "eslint-loader": "^3.0.2",
        "eslint-plugin-jest": "23.0.5",
        "eslint-plugin-tui": "file:totara/core/tui/scripts/eslint/plugin",
        "eslint-plugin-vue": "5.2.3",
        "gherkin-lint": "2.11.1",
        "grunt": "1.0.1",
        "grunt-contrib-less": "1.3.0",
        "grunt-contrib-uglify": "1.0.1",
        "grunt-contrib-watch": "1.0.0",
        "grunt-eslint": "20.1.0",
        "grunt-postcss": "^0.8.0",
        "grunt-stylelint": "0.12.0",
        "jest": "^24.8.0",
        "jest-extended": "^0.11.2",
        "jest-junit": "^9.0.0",
        "jest-serializer-vue": "^2.0.2",
        "jest-transform-stub": "^2.0.0",
        "jest-watch-typeahead": "^0.4.0",
        "lint-staged": "^9.5.0",
        "loader-utils": "^1.2.3",
        "mini-css-extract-plugin": "^0.8.0",
        "minimatch": "^3.0.4",
        "prettier": "1.19.1",
        "null-loader": "^3.0.0",
        "rtlcss": "^2.0.5",
        "run-node": "^1.0.0",
        "semver": "5.3.0",
        "shifter": "0.5.0",
        "stylelint": "12.0.0",
        "stylelint-checkstyle-formatter": "0.1.2",
        "stylelint-config-prettier": "7.0.0",
        "stylelint-order": "3.1.1",
        "tiny-glob": "^0.2.6",
        "vue-jest": "4.0.0-beta.2",
        "vue-loader": "^15.7.0",
        "vue-template-compiler": "^2.6.10",
        "webpack": "4.41.2",
        "webpack-cli": "^3.3.9",
        "xmldom": "0.1.22",
        "xpath": "0.0.23",
        "yargs": "^15.0.2"
    },
    "engines": {
        "node": "^8.10.0"
    }
}
